{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "test": "NODE_ENV=test mocha ./lib/**/__tests__/*.js ./pages/**/__tests__/*.js ./components/**/__tests__/*.js -R spec --compilers js:babel-register",
    "dev": "nodemon lib/server.js --exec babel-node --watch lib --ignore __tests__",
    "build": "next build && babel lib -d dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "babel-register": "^6.24.0",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "mocha": "^3.2.0",
    "next": "beta",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-preset-es2015": "^6.24.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.2",
    "fetch-mock": "^5.9.4",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.4.2"
  }
}
