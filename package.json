{
  "name": "web",
  "version": "0.0.0",
  "scripts": {
    "test": "mocha '**/__tests__/*' -R spec --compilers js:babel-register",
    "dev": "nodemon lib/server.js --exec babel-node --watch lib --ignore __tests__",
    "build": "next build && babel lib -d dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "babel-register": "^6.24.0",
    "express": "^4.15.2",
    "mocha": "^3.2.0",
    "next": "beta",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "nodemon": "^1.11.0"
  }
}
